  "devDependencies": {
    "chai": "~3.5.0",
    "chai-as-promised": "~5.3.0",
    "html-webpack-plugin": "~2.17.0",
    "jsdom": "~9.2.1",
    "jsdom-global": "~2.0.0",
    "mocha": "~2.5.3",
    "nock": "~8.0.0",
    "path": "~0.12.7",
    "sinon": "~1.17.4",
    "sinon-chai": "~2.8.0",
    "source-map-loader": "~0.1.5",
    "tsconfig-glob": "~0.4.3",
    "tslint": "~3.10.2",
    "ts-loader": "~0.8.2",
    "ts-node": "~0.7.3",
    "typings": "~1.0.4",
    "typescript": "~1.8.10",
    "webpack": "~1.13.1",
    "webpack-dev-server": "~1.14.1"
  },
  "scripts": {
    "tsconfig": "./node_modules/tsconfig-glob/cli .",
    "prepare": "npm install && typings install && npm run tsconfig",
    "linter": "./node_modules/tslint/bin/tslint src/**/*.ts",
    "start": "webpack-dev-server --config webpack.dev.conf.js",
    "clean": "npm prune && rm -rf node_modules typings lib",
    "test": "npm run linter && ./node_modules/mocha/bin/mocha --require jsdom-global/register --compilers ts:ts-node/register test/**/*.specs.ts"
  },
  "dependencies": {
    "core-decorators": "^0.12.3"
  }
